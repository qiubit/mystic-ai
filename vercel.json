{
  "version": 2,
  "functions": {
    "api/tarot-reading.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/tarot-reading",
      "dest": "/api/tarot-reading.js"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, no-cache" },
        { "key": "Connection", "value": "keep-alive" }
      ]
    }
  ]
}